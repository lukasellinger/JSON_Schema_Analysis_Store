{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "transferOption.json#",
  "type": "object",
  "title": "transferOption",
  "description": "Describes method and media by which a resource is obtained from the distributor.",
  "translation": {
    "ISO 19115-2": [
      "MD_Distributor \u003e distributorTransferOptions \u003e MD_DigitalTransferOptions"
    ],
    "FGDC CSDGM": [
      "distinfo \u003e stdorder \u003e digform \u003e digtopt"
    ]
  },
  "example": "../examples/transferOption.json",
  "additionalProperties": true,
  "properties": {
    "transferSize": {
      "type": "number",
      "description": "Estimated size of a unit in the specified transfer format, expressed in megabytes."
    },
    "unitsOfDistribution": {
      "type": "string",
      "description": "The tiles, layers, geographic areas, etc., in which data are available."
    },
    "onlineOption": {
      "type": "array",
      "description": "Information about obtaining the resource on-line.",
      "translation": {
        "ISO 19115-2": [
          "MD_DigitalTransferOptions \u003e online \u003e CI_OnlineResource"
        ],
        "FGDC CSDGM": [
          "distinfo \u003e stdorder \u003e digform \u003e digtopt \u003e onlinopt"
        ]
      },
      "items": {
        "$ref": "onlineResource.json#"
      },
      "uniqueItems": true
    },
    "offlineOption": {
      "type": "array",
      "description": "Information about obtaining the resource through off-line procedure.",
      "translation": {
        "ISO 19115-2": [
          "MD_DigitalTransferOptions \u003e offline \u003e MD_Medium"
        ],
        "FGDC CSDGM": [
          "distinfo \u003e stdorder \u003e digform \u003e digtopt \u003e offoptn"
        ]
      },
      "additionalProperties": true,
      "items": {
        "$ref": "./medium.json#"
      }
    },
    "transferFrequency": {
      "description": "The rate of occurrence of distribution.",
      "translation": {},
      "$ref": "./timePeriod.json#/definitions/duration"
    },
    "distributionFormat": {
      "type": "array",
      "description": "Provides information about the format used by the distributor.",
      "translation": {
        "ISO 19115-2": [
          "MD_Distribution \u003e distributor \u003e MD_Distributor \u003e distributorFormat \u003e MD_Format"
        ],
        "FGDC CSDGM": [
          "distinfo \u003e stdorder \u003e digform"
        ]
      },
      "items": {
        "$ref": "./format.json#"
      }
    }
  }
}