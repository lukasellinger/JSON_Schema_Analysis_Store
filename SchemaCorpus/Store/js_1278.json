{
  "_priv": {
    "bytes": {
      "type": "string",
      "pattern": "^\\d+[KkMmGg]$"
    }
  },
  "parameter": {
    "type": "string",
    "pattern": "^[a-zA-Z0-9_-]+\u003d.+$"
  },
  "memory": {
    "$ref": "#/_priv/bytes"
  },
  "disk": {
    "$ref": "#/_priv/bytes"
  },
  "cpu": {
    "type": "string",
    "pattern": "^\\d+%$"
  },
  "time_interval": {
    "type": "string",
    "pattern": "^\\d+[SsMmHhDdMmYy]$"
  },
  "proid": {
    "type": "string",
    "minLength": 2,
    "maxLength": 20,
    "pattern": "^[a-zA-Z0-9_-]+$"
  },
  "ticket": {
    "type": "string",
    "maxLength": 32,
    "pattern": "^[\\w\\-]+@[\\w\\.]+$"
  },
  "keytab": {
    "type": "string",
    "maxLength": 128,
    "pattern": "^[\\w\\-]+#[\\w\\-\\.]+@[\\w\\.]+$"
  },
  "endpoint": {
    "type": "string",
    "maxLength": 20,
    "pattern": "^[\\w\\-]+$"
  },
  "cell": {
    "type": "string",
    "maxLength": 20,
    "pattern": "^[a-zA-Z0-9-]+$"
  },
  "system_id": {
    "type": "integer",
    "minimum": 0
  },
  "port": {
    "type": "integer",
    "minimum": 0,
    "maximum": 65535
  },
  "rank": {
    "type": "integer",
    "minimum": 0,
    "maximum": 100
  },
  "rank_adjustment": {
    "type": "integer",
    "minimum": 0,
    "maximum": 100
  },
  "max_utilization": {
    "type": "number",
    "minimum": 0,
    "maximum": 100
  },
  "environment": {
    "type": "string",
    "pattern": "^dev$|^qa$|^uat$|^prod$"
  },
  "location": {
    "type": "string",
    "pattern": "^[a-zA-Z]+$"
  },
  "key_value": {
    "type": "string",
    "maxLength": 128,
    "pattern": "^[a-zA-Z0-9-_]+\u003d[a-zA-Z0-9\\-_\\.]+$"
  },
  "app_user": {
    "type": "string",
    "pattern": "^([\\w\\-]+)@([\\w\\-]+)$"
  },
  "app_id": {
    "anyOf": [
      {
        "type": "string",
        "maxLength": 117,
        "pattern": "^[a-zA-Z0-9_-]{2,20}([.][\\w-]+)+$"
      },
      {
        "type": "string",
        "maxLength": 117,
        "pattern": "^[a-zA-Z0-9_-]{2,20}@[\\w-]+([.][\\w-]+)+$"
      }
    ]
  },
  "appmonitor_id": {
    "type": "string",
    "maxLength": 128,
    "pattern": "^[^:]+:[^:]+$"
  },
  "instance_id": {
    "anyOf": [
      {
        "type": "string",
        "maxLength": 128,
        "pattern": "^[a-zA-Z0-9_-]{2,20}([.][\\w-]+)+#\\d+$"
      },
      {
        "type": "string",
        "maxLength": 128,
        "pattern": "^[a-zA-Z0-9_-]{2,20}@[\\w-]+([.][\\w-]+)+#\\d+$"
      }
    ]
  },
  "priority": {
    "type": "integer",
    "minimum": 0,
    "maximum": 100
  },
  "identity_group_id": {
    "anyOf": [
      {
        "type": "string",
        "maxLength": 128,
        "pattern": "^[\\w\\-]+(\\.[\\w\\-]+)+$"
      },
      {
        "type": "string",
        "maxLength": 128,
        "pattern": "^([\\w\\-]+)@([\\w\\-]+)(\\.[\\w\\-]+)+$"
      }
    ]
  },
  "trait": {
    "type": "string",
    "maxLength": 32
  },
  "partition": {
    "anyOf": [
      {
        "type": "string",
        "maxLength": 32
      },
      {
        "type": "null"
      }
    ]
  },
  "label": {
    "anyOf": [
      {
        "type": "string",
        "maxLength": 32
      },
      {
        "type": "null"
      }
    ]
  },
  "hostport": {
    "type": "string",
    "pattern": "^[a-zA-Z0-9\\-_]+(\\.[a-zA-Z0-9\\-_^#]+)+:[1-9][0-9]*$"
  },
  "user": {
    "type": "string",
    "pattern": "^[\\w\\-]+(@[\\w\\-\\.]+)?$"
  },
  "dns_alias": {
    "type": "string",
    "pattern": "^[a-zA-Z0-9]+(\\.[a-zA-Z0-9\\-]+)+$"
  }
}