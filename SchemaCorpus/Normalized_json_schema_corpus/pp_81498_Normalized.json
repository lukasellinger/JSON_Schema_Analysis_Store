{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://integracja.firefighters.ki.agh.edu.pl",
  "description": "RQ from firefightersPost, RS from notification center",
  "type": "object",
  "properties": {
    "notificationId": {
      "type": "string"
    },
    "postName": {
      "type": "string"
    },
    "actionName": {
      "type": "string"
    },
    "accidentType": {
      "type": "string"
    },
    "community": {
      "type": "string"
    },
    "areaSize": {
      "type": "string"
    },
    "objectName": {
      "type": "string"
    },
    "owner": {
      "type": "string"
    },
    "otherDamages": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "damageName": {
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "additionalItems": false
    },
    "creator": {
      "type": "string"
    },
    "usedEquipment": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "workTimeInH": {
            "type": "integer"
          },
          "fuelType": {
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "additionalItems": false
    },
    "firefighters": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "surname": {
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "additionalItems": false
    },
    "brigades": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "membersNumber": {
            "type": "integer"
          },
          "distanceInKm": {
            "type": "integer"
          },
          "departureTime": {
            "format": "date-time"
          },
          "arrivalTime": {
            "format": "date-time"
          },
          "pumpworkTime": {
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "additionalItems": false
    }
  },
  "additionalProperties": false
}