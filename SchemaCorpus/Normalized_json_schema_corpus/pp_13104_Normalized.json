{
  "@context": "http://qiicr.org/dcmqi/contexts/dcmqi.jsonld",
  "id": "https://raw.githubusercontent.com/qiicr/dcmqi/master/doc/schemas/seg-schema.json#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "additionalProperties": false,
  "properties": {
    "@schema": {
      "type": "string"
    },
    "ContentCreatorName": {
      "allOf": [
        {
          "$ref": "#/definitions/common-schema.json_definitions_PN"
        }
      ],
      "default": "Reader1"
    },
    "ClinicalTrialSeriesID": {
      "allOf": [
        {
          "$ref": "#/definitions/common-schema.json_definitions_LO"
        }
      ],
      "default": "Session1"
    },
    "ContentLabel": {
      "allOf": [
        {
          "$ref": "#/definitions/common-schema.json_definitions_CS"
        }
      ],
      "default": "SEGMENTATION"
    },
    "ContentDescription": {
      "allOf": [
        {
          "$ref": "#/definitions/common-schema.json_definitions_LO"
        }
      ],
      "default": "Image segmentation"
    },
    "ClinicalTrialCoordinatingCenterName": {
      "allOf": [
        {
          "$ref": "#/definitions/common-schema.json_definitions_LO"
        }
      ],
      "default": "dcmqi"
    },
    "ClinicalTrialTimePointID": {
      "allOf": [
        {
          "$ref": "#/definitions/common-schema.json_definitions_LO"
        }
      ],
      "default": "1"
    },
    "SeriesDescription": {
      "allOf": [
        {
          "$ref": "#/definitions/common-schema.json_definitions_LO"
        }
      ],
      "default": "Segmentation"
    },
    "SeriesNumber": {
      "allOf": [
        {
          "$ref": "#/definitions/common-schema.json_definitions_IS"
        }
      ],
      "default": "300"
    },
    "InstanceNumber": {
      "allOf": [
        {
          "$ref": "#/definitions/common-schema.json_definitions_IS"
        }
      ],
      "default": "1"
    },
    "BodyPartExamined": {
      "allOf": [
        {
          "$ref": "#/definitions/common-schema.json_definitions_CS"
        }
      ],
      "default": ""
    },
    "segmentAttributesFileMapping": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string"
      }
    },
    "segmentAttributes": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "array",
        "minItems": 1,
        "items": {
          "type": "object",
          "additionalProperties": false,
          "required": [
            "labelID",
            "SegmentedPropertyCategoryCodeSequence",
            "SegmentedPropertyTypeCodeSequence",
            "SegmentAlgorithmType"
          ],
          "properties": {
            "labelID": {
              "type": "integer",
              "default": 1
            },
            "SegmentDescription": {
              "type": "string"
            },
            "SegmentLabel": {
              "$ref": "#/definitions/common-schema.json_definitions_LO"
            },
            "SegmentedPropertyCategoryCodeSequence": {
              "allOf": [
                {
                  "$ref": "#/definitions/common-schema.json_definitions_codeSequence"
                }
              ],
              "default": {
                "CodeValue": "85756007",
                "CodingSchemeDesignator": "SCT",
                "CodeMeaning": "Tissue"
              }
            },
            "SegmentedPropertyTypeCodeSequence": {
              "allOf": [
                {
                  "$ref": "#/definitions/common-schema.json_definitions_codeSequence"
                }
              ],
              "default": {
                "CodeValue": "85756007",
                "CodingSchemeDesignator": "SCT",
                "CodeMeaning": "Tissue"
              }
            },
            "SegmentedPropertyTypeModifierCodeSequence": {
              "$ref": "#/definitions/common-schema.json_definitions_codeSequence"
            },
            "AnatomicRegionSequence": {
              "$ref": "#/definitions/common-schema.json_definitions_codeSequence"
            },
            "AnatomicRegionModifierSequence": {
              "$ref": "#/definitions/common-schema.json_definitions_codeSequence"
            },
            "SegmentAlgorithmType": {
              "allOf": [
                {
                  "$ref": "#/definitions/common-schema.json_definitions_CS"
                },
                {
                  "enum": [
                    "AUTOMATIC",
                    "MANUAL",
                    "SEMIAUTOMATIC"
                  ]
                }
              ],
              "default": "SEMIAUTOMATIC"
            },
            "SegmentAlgorithmName": {
              "$ref": "#/definitions/common-schema.json_definitions_LO"
            },
            "recommendedDisplayRGBValue": {
              "$ref": "#/definitions/common-schema.json_definitions_RGB"
            },
            "RecommendedDisplayCIELabValue": {
              "$ref": "#/definitions/common-schema.json_definitions_CIELab"
            },
            "TrackingIdentifier": {
              "$ref": "#/definitions/common-schema.json_definitions_UT"
            },
            "TrackingUniqueIdentifier": {
              "$ref": "#/definitions/common-schema.json_definitions_UI"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "common-schema.json_definitions_PN": {
      "type": "string",
      "maxLength": 64
    },
    "common-schema.json_definitions_LO": {
      "type": "string",
      "maxLength": 64
    },
    "common-schema.json_definitions_CS": {
      "type": "string",
      "maxLength": 16
    },
    "common-schema.json_definitions_IS": {
      "type": "string",
      "maxLength": 12
    },
    "common-schema.json_definitions_codeSequence": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "CodeValue",
        "CodingSchemeDesignator",
        "CodeMeaning"
      ],
      "properties": {
        "CodeValue": {
          "$ref": "#/definitions/common-schema.json_definitions_SH",
          "default": "85756007"
        },
        "CodingSchemeDesignator": {
          "$ref": "#/definitions/common-schema.json_definitions_SH",
          "default": "SCT"
        },
        "CodeMeaning": {
          "$ref": "#/definitions/common-schema.json_definitions_LO",
          "default": "Tissue"
        }
      }
    },
    "common-schema.json_definitions_SH": {
      "type": "string",
      "maxLength": 64
    },
    "common-schema.json_definitions_RGB": {
      "type": "array",
      "items": {
        "type": "integer",
        "minimum": 0,
        "maximum": 255
      },
      "minItems": 3,
      "maxItems": 3
    },
    "common-schema.json_definitions_CIELab": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/common-schema.json_definitions_US"
      },
      "minItems": 3,
      "maxItems": 3
    },
    "common-schema.json_definitions_US": {
      "type": "integer",
      "minimum": 0,
      "maximum": 2E+16
    },
    "common-schema.json_definitions_UT": {
      "type": "string"
    },
    "common-schema.json_definitions_UI": {
      "type": "string",
      "pattern": "[0-9\\.]*"
    }
  }
}