{
  "name": "iFlux Slack bot",
  "public": true,
  "organizationId": 1,
  "configuration": {
    "schema": {
      "$schema": "http://json-schema.org/draft-04/schema#",
      "type": "object",
      "properties": {
        "botId": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "required": [
        "botId"
      ]
    },
    "url": "http://slackgateway.bot.channel/configuration",
    "token": "\u003cJSON Web Token\u003e"
  },
  "configurationUi": {
    "schemaForm": [
      "sensorId",
      {
        "type": "submit",
        "style": "btn-primary",
        "title": "Submit"
      }
    ]
  },
  "target": {
    "url": "http://slackgateway.bot.channel/actions",
    "token": "\u003cJSON Web Token\u003e"
  }
}