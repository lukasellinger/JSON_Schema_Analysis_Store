{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "#/",
  "type": "object",
  "description": "schema definitions for ONVIF profile S",
  "onvif": {
    "ptz": {
      "AbsoluteMoveArg": {
        "$ref": "#/definitions/onvif_ptz_moveArgs"
      },
      "AbsoluteMoveResult": {
        "type": "object"
      },
      "RelativeMoveArg": {
        "$ref": "#/definitions/onvif_ptz_moveArgs"
      },
      "RelativeMoveResult": {
        "type": "object"
      },
      "ContinuousMoveArg": {
        "type": "object",
        "properties": {
          "x": {
            "type": "number",
            "maximum": 1,
            "minimum": 0
          },
          "y": {
            "type": "number",
            "maximum": 1,
            "minimum": 0
          },
          "zoom": {
            "type": "number",
            "maximum": 1,
            "minimum": 0
          },
          "timeout": {
            "type": "number"
          }
        },
        "required": [
          "x",
          "y"
        ]
      },
      "ContinuousMoveResult": {
        "type": "object"
      },
      "GetPresetsArg": {
        "type": "object"
      },
      "GetPresetsResult": {
        "type": "object"
      },
      "GetNodesArg": {
        "type": "object"
      },
      "GetNodesResult": {
        "type": "object"
      },
      "GotoPresetArg": {
        "type": "object",
        "properties": {
          "preset": {
            "type": "string"
          },
          "speed": {
            "$ref": "#/definitions/onvif_ptz_speed"
          }
        },
        "required": [
          "preset"
        ]
      },
      "GotoPresetResult": {
        "type": "object"
      },
      "StopArg": {
        "type": "object",
        "properties": {
          "panTilt": {
            "type": "boolean"
          },
          "zoom": {
            "type": "boolean"
          }
        }
      },
      "StopResult": {
        "type": "object"
      },
      "PTZStatusArg": {
        "type": "object"
      },
      "PTZStatusResult": {
        "type": "object",
        "properties": {
          "position": {
            "type": "object",
            "properties": {
              "x": {
                "type": "number"
              },
              "y": {
                "type": "number"
              },
              "zoom": {
                "type": "number"
              }
            }
          },
          "moveStatus": {
            "type": "object",
            "properties": {
              "PanTilt": {
                "type": "string",
                "enum": [
                  "IDLE",
                  "MOVING",
                  "UNKNOWN"
                ]
              },
              "Zoom": {
                "type": "string",
                "enum": [
                  "IDLE",
                  "MOVING",
                  "UNKNOWN"
                ]
              }
            }
          },
          "error": {
            "type": "string"
          },
          "utcTime": {
            "type": [
              "string",
              "number"
            ]
          }
        }
      },
      "speed": {
        "type": "object",
        "properties": {
          "x": {
            "type": "number",
            "maximum": 1,
            "minimum": 0
          },
          "y": {
            "type": "number",
            "maximum": 1,
            "minimum": 0
          },
          "zoom": {
            "type": "number",
            "maximum": 1,
            "minimum": 0
          }
        },
        "required": [
          "x",
          "y"
        ]
      },
      "moveArgs": {
        "type": "object",
        "properties": {
          "x": {
            "type": "number",
            "maximum": 1,
            "minimum": -1
          },
          "y": {
            "type": "number",
            "maximum": 1,
            "minimum": -1
          },
          "zoom": {
            "type": "number",
            "maximum": 1,
            "minimum": 0
          },
          "speed": {
            "$ref": "#/definitions/onvif_ptz_speed"
          }
        },
        "required": [
          "x",
          "y"
        ]
      },
      "SpaceSelector": {
        "oneOf": [
          {
            "type": "string",
            "pattern": "^http://www.onvif.org/ver10/tptz/PanTiltSpaces/PositionGenericSpace$"
          },
          {
            "type": "string",
            "pattern": "^http://www.onvif.org/ver10/tptz/PanTiltSpaces/VelocityGenericSpace$"
          },
          {
            "type": "string",
            "pattern": "^http://www.onvif.org/ver10/tptz/PanTiltSpaces/TranslationGenericSpace$"
          },
          {
            "type": "string",
            "pattern": "^http://www.onvif.org/ver10/tptz/PanTiltSpaces/GenericSpeedSpace$"
          }
        ]
      }
    },
    "media": {
      "getStreamUriArg": {
        "type": "object",
        "properties": {
          "streamType": {
            "type": "string",
            "enum": [
              "RTP-Unicast",
              "RTP-Multicast",
              "MPEG"
            ],
            "default": "MPEG"
          },
          "transport": {
            "type": "string",
            "enum": [
              "UDP",
              "TCP",
              "RTSP",
              "HTTP",
              "WebSocket"
            ],
            "default": "WebSocket"
          }
        },
        "required": [
          "streamType",
          "transport"
        ]
      },
      "getStreamUriResult": {
        "type": "object",
        "properties": {
          "streamUri": {
            "type": "string"
          }
        }
      },
      "getSnapshotUriArg": {
        "type": "object"
      },
      "getSnapshotUriResult": {
        "type": "object",
        "properties": {
          "snapshotUri": {
            "type": "string"
          }
        }
      }
    }
  },
  "definitions": {
    "onvif_ptz_moveArgs": {
      "type": "object",
      "properties": {
        "x": {
          "type": "number",
          "maximum": 1,
          "minimum": -1
        },
        "y": {
          "type": "number",
          "maximum": 1,
          "minimum": -1
        },
        "zoom": {
          "type": "number",
          "maximum": 1,
          "minimum": 0
        },
        "speed": {
          "$ref": "#/definitions/onvif_ptz_speed"
        }
      },
      "required": [
        "x",
        "y"
      ]
    },
    "onvif_ptz_speed": {
      "type": "object",
      "properties": {
        "x": {
          "type": "number",
          "maximum": 1,
          "minimum": 0
        },
        "y": {
          "type": "number",
          "maximum": 1,
          "minimum": 0
        },
        "zoom": {
          "type": "number",
          "maximum": 1,
          "minimum": 0
        }
      },
      "required": [
        "x",
        "y"
      ]
    }
  }
}