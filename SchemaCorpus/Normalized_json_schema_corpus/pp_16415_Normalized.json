        "columns": {
          "type": "array",
          "items": {
            "type": "string",
            "title": "The 0th Schema ",
            "default": "",
            "examples": [
              "id",
              "iso_code3",
              "country",
              "indc_text",
              "status",
              "climate_response",
              "type_of_information",
              "sector",
              "target_number",
              "target",
