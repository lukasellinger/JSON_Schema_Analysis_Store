      "pbj": {
        "type": "identifier",
        "rule": "single"
      }
    },
    "_id": {
      "type": "string",
      "pattern": "^[\\w\\/\\.:-]+$",
      "pbj": {
        "type": "identifier",
        "rule": "single"
      }
