{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": {
    "allOf": [
      {
        "$ref": "#/definitions/..\\definitions\\gear.json_definitions_gear-doc"
      }
    ]
  },
  "definitions": {
    "..\\definitions\\gear.json_definitions_gear-doc": {
      "type": "object",
      "properties": {
        "_id": {
          "$ref": "#/definitions/..\\definitions\\common.json_definitions_objectid"
        },
        "category": {
          "$ref": "#/definitions/..\\definitions\\gear.json_definitions_gear-category"
        },
        "gear": {
          "$ref": "#/definitions/..\\definitions\\gear.json_definitions_gear"
        },
        "exchange": {
          "$ref": "#/definitions/..\\definitions\\gear.json_definitions_gear-exchange"
        },
        "created": {
          "$ref": "#/definitions/..\\definitions\\created-modified.json_definitions_created"
        },
        "modified": {
          "$ref": "#/definitions/..\\definitions\\created-modified.json_definitions_modified"
        }
      },
      "description": "A full gear description, including manifest and exchange information"
    },
    "..\\definitions\\common.json_definitions_objectid": {
      "type": "string",
      "pattern": "^[a-fA-F0-9]{24}$"
    },
    "..\\definitions\\gear.json_definitions_gear-category": {
      "type": "string",
      "description": "The gear category"
    },
    "..\\definitions\\gear.json_definitions_gear": {
      "type": "object",
      "description": "Gear manifest",
      "properties": {
        "author": {
          "$ref": "#/definitions/..\\definitions\\gear.json_definitions_gear-author"
        },
        "maintainer": {
          "$ref": "#/definitions/..\\definitions\\gear.json_definitions_gear-maintainer"
        },
        "cite": {
          "$ref": "#/definitions/..\\definitions\\gear.json_definitions_gear-cite"
        },
        "config": {
          "$ref": "#/definitions/..\\definitions\\gear.json_definitions_gear-config"
        },
        "custom": {
          "$ref": "#/definitions/..\\definitions\\gear.json_definitions_gear-custom"
        },
        "description": {
          "$ref": "#/definitions/..\\definitions\\gear.json_definitions_gear-description"
        },
        "environment": {
          "$ref": "#/definitions/..\\definitions\\gear.json_definitions_gear-environment"
        },
        "command": {
          "$ref": "#/definitions/..\\definitions\\gear.json_definitions_gear-command"
        },
        "inputs": {
          "$ref": "#/definitions/..\\definitions\\gear.json_definitions_gear-inputs"
        },
        "label": {
          "$ref": "#/definitions/..\\definitions\\gear.json_definitions_gear-label"
        },
        "license": {
          "$ref": "#/definitions/..\\definitions\\gear.json_definitions_gear-license"
        },
        "name": {
          "$ref": "#/definitions/..\\definitions\\gear.json_definitions_gear-name"
        },
        "source": {
          "$ref": "#/definitions/..\\definitions\\gear.json_definitions_gear-source"
        },
        "url": {
          "$ref": "#/definitions/..\\definitions\\gear.json_definitions_gear-url"
        },
        "version": {
          "$ref": "#/definitions/..\\definitions\\gear.json_definitions_gear-version"
        }
      },
      "required": [
        "author",
        "config",
        "description",
        "inputs",
        "label",
        "license",
        "name",
        "source",
        "url",
        "version"
      ],
      "x-sdk-include-empty": [
        "config",
        "inputs"
      ],
      "additionalProperties": false
    },
    "..\\definitions\\gear.json_definitions_gear-author": {
      "type": "string",
      "description": "The author of this gear."
    },
    "..\\definitions\\gear.json_definitions_gear-maintainer": {
      "type": "string",
      "description": "(optional) The maintainer of this gear. Can be used to distinguish the algorithm author from the gear maintainer."
    },
    "..\\definitions\\gear.json_definitions_gear-cite": {
      "type": "string",
      "description": "(optional) Any citations relevant to the algorithm(s) or work present in the gear."
    },
    "..\\definitions\\gear.json_definitions_gear-config": {
      "type": "object",
      "additionalProperties": {
        "$ref": "#/definitions/..\\definitions\\gear.json_definitions_gear-directive"
      },
      "description": "Schema snippets describing the options this gear consumes. Not currently processed."
    },
    "..\\definitions\\gear.json_definitions_gear-directive": {
      "type": "object",
      "description": "A schema directive."
    },
    "..\\definitions\\gear.json_definitions_gear-custom": {
      "type": "object",
      "description": "A place for gear authors to put arbitrary information."
    },
    "..\\definitions\\gear.json_definitions_gear-description": {
      "type": "string",
      "description": "A brief description of the gear\u0027s purpose. Ideally 1-4 sentences."
    },
    "..\\definitions\\gear.json_definitions_gear-environment": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      },
      "description": "Environment variables that should be set for the gear."
    },
    "..\\definitions\\gear.json_definitions_gear-command": {
      "type": "string",
      "description": "If provided, the starting command for the gear, rather than /flywheel/v0/run. Will be templated according to the spec."
    },
    "..\\definitions\\gear.json_definitions_gear-inputs": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "additionalProperties": {
          "$ref": "#/definitions/..\\definitions\\gear.json_definitions_gear-input-item"
        }
      },
      "description": "Schema snippets describing the inputs this gear consumes."
    },
    "..\\definitions\\gear.json_definitions_gear-input-item": {
      "description": "Describes a gear input",
      "properties": {
        "base": {
          "type": "string",
          "enum": [
            "file",
            "api-key"
          ],
          "description": "The type of gear input."
        },
        "description": {
          "type": "string",
          "description": "Hackaround for description not technically being a schema directive"
        },
        "optional": {
          "type": "boolean",
          "description": "Allow the gear to mark an input file as optional."
        }
      },
      "required": [
        "base"
      ],
      "additionalProperties": {
        "$ref": "#/definitions/..\\definitions\\gear.json_definitions_gear-directive"
      }
    },
    "..\\definitions\\gear.json_definitions_gear-label": {
      "type": "string",
      "description": "The human-friendly name of this gear."
    },
    "..\\definitions\\gear.json_definitions_gear-license": {
      "type": "string",
      "description": "Software license of the gear"
    },
    "..\\definitions\\gear.json_definitions_gear-name": {
      "type": "string",
      "pattern": "^[a-z0-9\\-]+$",
      "description": "The identification of this gear."
    },
    "..\\definitions\\gear.json_definitions_gear-source": {
      "$ref": "#/definitions/..\\definitions\\gear.json_definitions_gear-uri",
      "description": "The URL where the source code of this gear can be found. Leave blank if none."
    },
    "..\\definitions\\gear.json_definitions_gear-uri": {
      "type": "string",
      "description": "A valid URI, or empty string."
    },
    "..\\definitions\\gear.json_definitions_gear-url": {
      "$ref": "#/definitions/..\\definitions\\gear.json_definitions_gear-uri",
      "description": "The URL where more information about this gear can be found. Leave blank if none."
    },
    "..\\definitions\\gear.json_definitions_gear-version": {
      "type": "string",
      "description": "A human-friendly string explaining the release version of this gear. Example: 3.2.1"
    },
    "..\\definitions\\gear.json_definitions_gear-exchange": {
      "type": "object",
      "description": "Metadata object that describes the origin of a gear version",
      "properties": {
        "git-commit": {
          "type": "string",
          "description": "The SHA-1 hash referring to the git commit"
        },
        "rootfs-hash": {
          "type": "string",
          "description": "The cryptographic hash of the root filesystem in the form of \"algorithm:\u003cbase16 hash\u003e\""
        },
        "rootfs-url": {
          "type": "string",
          "description": "The absolute URL of the gear\u0027s root file system"
        }
      },
      "required": [
        "git-commit",
        "rootfs-hash",
        "rootfs-url"
      ]
    },
    "..\\definitions\\created-modified.json_definitions_created": {
      "type": "string",
      "format": "date-time"
    },
    "..\\definitions\\created-modified.json_definitions_modified": {
      "type": "string",
      "format": "date-time"
    }
  }
}