{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "hitSamples",
    "activeSessions",
    "startedSessions",
    "endedSessions"
  ],
  "properties": {
    "hitSamples": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "ua",
          "ip",
          "ts"
        ],
        "properties": {
          "ua": {
            "type": "string"
          },
          "ip": {
            "type": "string"
          },
          "host": {
            "type": "string"
          },
          "url": {
            "type": "string"
          },
          "ts": {
            "type": "string"
          }
        }
      }
    },
    "activeSessions": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "ua",
          "ip",
          "started"
        ],
        "properties": {
          "ua": {
            "type": "string"
          },
          "ip": {
            "type": "string"
          },
          "started": {
            "type": "string"
          }
        }
      }
    },
    "startedSessions": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "ua",
          "ip",
          "started"
        ],
        "properties": {
          "ua": {
            "type": "string"
          },
          "ip": {
            "type": "string"
          },
          "started": {
            "type": "string"
          }
        }
      }
    },
    "endedSessions": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "ua",
          "ip",
          "started",
          "ended"
        ],
        "properties": {
          "ua": {
            "type": "string"
          },
          "ip": {
            "type": "string"
          },
          "started": {
            "type": "string"
          },
          "ended": {
            "type": "string"
          }
        }
      }
    }
  }
}