{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "/",
  "type": "object",
  "title": "oxAttributes",
  "additionalProperties": true,
  "properties": {
    "tlsClientAuthSubjectDn": {
      "type": "string"
    },
    "runIntrospectionScriptBeforeAccessTokenAsJwtCreationAndIncludeClaims": {
      "description": "Run Introspection Script Before Access Token As Jwt Creation And Include Claims.",
      "type": "boolean"
    },
    "keepClientAuthorizationAfterExpiration": {
      "description": "Keep Client Authorization After Expiration.",
      "type": "boolean"
    },
    "allowSpontaneousScopes": {
      "description": "Whether spontaneous scopes are allowed",
      "type": "boolean"
    },
    "spontaneousScopes": {
      "type": "array",
      "minItems": 0,
      "uniqueItems": true,
      "additionalItems": true,
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "spontaneousScopeScriptDns": {
      "type": "array",
      "minItems": 0,
      "uniqueItems": true,
      "additionalItems": true,
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "backchannelLogoutSessionRequired": {
      "description": "Back channel Logout Session Required.",
      "type": "boolean"
    },
    "backchannelLogoutUri": {
      "type": "array",
      "minItems": 0,
      "uniqueItems": true,
      "additionalItems": true,
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "additionalAudience": {
      "type": "array",
      "minItems": 0,
      "uniqueItems": true,
      "additionalItems": true,
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "postAuthnScripts": {
      "type": "array",
      "minItems": 0,
      "uniqueItems": true,
      "additionalItems": true,
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "consentGatheringScripts": {
      "type": "array",
      "minItems": 0,
      "uniqueItems": true,
      "additionalItems": true,
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "introspectionScripts": {
      "type": "array",
      "minItems": 0,
      "uniqueItems": true,
      "additionalItems": true,
      "items": {
        "type": "string",
        "minLength": 1
      }
    }
  }
}