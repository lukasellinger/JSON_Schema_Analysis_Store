{
  "id": "pin.json",
  "title": "pin",
  "description": "Schema definition of Pinterest Pin",
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "link": {
      "type": "string",
      "format": "uri"
    },
    "url": {
      "type": "string",
      "format": "uri"
    },
    "creator": {
      "type": "object",
      "additionalProperties": {
        "$ref": "#/definitions/user.json"
      }
    },
    "board": {
      "$ref": "#/definitions/board.json"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "note": {
      "type": "string"
    },
    "color": {
      "type": "string"
    },
    "counts": {
      "type": "object",
      "additionalProperties": {
        "type": "integer"
      }
    },
    "media": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "attribution": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "description": {
      "type": "string"
    },
    "image": {
      "$ref": "#/definitions/image.json"
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "visual_search_attrs": {
      "type": "object"
    },
    "attribution_objects": {
      "type": "array",
      "items": {
        "oneOf": [
          {
            "$ref": "#/definitions/board.json"
          },
          {
            "$ref": "#/definitions/user.json"
          }
        ]
      }
    },
    "in_stock": {
      "type": "integer",
      "enum": [
        {
          "default": -1,
          "description": "unknown"
        },
        {
          "default": 0,
          "description": "out_of_stock"
        },
        {
          "default": 1,
          "description": "in_stock"
        }
      ]
    }
  },
  "required": [
    "id",
    "created_at",
    "creator"
  ],
  "definitions": {
    "user.json": {
      "title": "user",
      "description": "Schema definition of Pinterest User",
      "$schema": "http://json-schema.org/schema#",
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "username": {
          "type": "string"
        },
        "first_name": {
          "type": "string"
        },
        "last_name": {
          "type": "string"
        },
        "bio": {
          "type": "string"
        },
        "created_at": {
          "type": "string",
          "format": "date-time"
        },
        "counts": {
          "type": "object",
          "additionalProperties": {
            "type": "integer"
          }
        },
        "email_frequency": {
          "type": "string",
          "enum": [
            {
              "default": "unset",
              "description": "unset"
            },
            {
              "default": "immediate",
              "description": "immediate"
            },
            {
              "default": "daily",
              "description": "daily"
            }
          ],
          "default": "unset"
        },
        "image": {
          "$ref": "#/definitions/image.json"
        }
      }
    },
    "image.json": {
      "title": "image",
      "description": "Schema definition of Pinterest image",
      "$schema": "http://json-schema.org/schema#",
      "type": "object",
      "properties": {
        "url": {
          "type": "string",
          "format": "uri"
        },
        "width": {
          "type": "integer"
        },
        "height": {
          "type": "integer"
        }
      }
    },
    "board.json": {
      "title": "board",
      "extends": {
        "$ref": "#/definitions/model.json"
      },
      "description": "Schema definition of Pinterest Board",
      "$schema": "http://json-schema.org/schema#",
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "url": {
          "type": "string",
          "format": "uri"
        },
        "creator_url": {
          "type": "string",
          "format": "uri"
        },
        "description": {
          "type": "string"
        },
        "creator": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "created_at": {
          "type": "string",
          "format": "date-time"
        },
        "contributors": {
          "type": "array",
          "unique": "true",
          "items": {
            "$ref": "#/definitions/user.json"
          }
        },
        "counts": {
          "type": "object",
          "additionalProperties": {
            "type": "integer"
          }
        },
        "image": {
          "$ref": "#/definitions/image.json"
        }
      },
      "required": [
        "image"
      ]
    },
    "model.json": {
      "$schema": "http://json-schema.org/schema#",
      "type": "object",
      "description": "Base model schema definition",
      "title": "model",
      "properties": {
        "id": {
          "type": "string"
        }
      },
      "required": []
    }
  }
}