          "maximum": 100,
          "min_length": 4,
          "type": [
            "string",
            "integer"
          ]
        },
        "screen_name": {
          "description": "screen name of user",
          "example": "gopher-1030",
          "pattern": "^[a-z0-9-_]{5,30}$",
          "type": "string"
