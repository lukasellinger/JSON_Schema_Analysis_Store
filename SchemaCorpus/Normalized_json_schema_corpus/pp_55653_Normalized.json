{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Peteramati configuration schema",
  "$defs": {
    "boolean_date": {
      "anyOf": [
        {
          "type": "boolean"
        },
        {
          "type": "integer",
          "exclusiveMinimum": 1000000000
        },
        {
          "type": "string"
        }
      ]
    },
    "pset_base": {
      "type": "object",
      "properties": {
        "psetid": {
          "description": "Unique database identifier for pset",
          "type": "integer",
          "exclusiveMinimum": 0
        },
        "key": {
          "description": "Unique string identifier for pset",
          "type": "string",
          "pattern": "^(?!.*_(?:rank|noextra|norm)$)[^_./\u0026;#][^/\u0026;#]*$"
        },
        "urlkey": {
          "description": "Preferred URL component for pset",
          "type": [
            "string",
            "integer"
          ],
          "pattern": "^[0-9A-Za-z][-0-9A-Za-z_.]*$"
        },
        "title": {
          "description": "Textual description of pset",
          "type": "string"
        },
        "category": {
          "description": "Name of pset group",
          "type": "string"
        },
        "weight": {
          "description": "Weight of pset within group",
          "type": "number",
          "minimum": 0
        },
        "disabled": {
          "description": "True if pset is not shown to users",
          "type": "boolean"
        },
        "admin_disabled": {
          "description": "True if pset is not shown to users or administrators",
          "type": "boolean"
        },
        "visible": {
          "description": "True if pset is shown to students",
          "$ref": "#/definitions/$defs_boolean_date"
        },
        "frozen": {
          "description": "True if students cannot update pset",
          "$ref": "#/definitions/$defs_boolean_date"
        },
        "anonymous": {},
        "gitless": {},
        "gitless_grades": {},
        "partner": {},
        "no_branch": {},
        "partner_repo": {},
        "hide_comments": {},
        "handout_repo_url": {},
        "handout_branch": {},
        "handout_hash": {},
        "handout_warn_hash": {},
        "directory": {},
        "test_file": {},
        "deadline": {},
        "grades": {},
        "grade_order": {},
        "grades_visible": {},
        "grade_statistics_visible": {},
        "grade_cdf_cutoff": {},
        "separate_extension_grades": {},
        "grade_script": {},
        "downloads": {},
        "runners": {},
        "default_runner": {},
        "runner_order": {},
        "run_dirpattern": {},
        "run_username": {},
        "run_overlay": {},
        "run_jailfiles": {},
        "run_xterm_js": {},
        "run_timeout": {},
        "run_skeletondir": {},
        "run_binddir": {},
        "diffs": {}
      }
    },
    "pset": {
      "$ref": "#/definitions/$defs_pset_base",
      "required": [
        "psetid",
        "key"
      ]
    },
    "grade": {
      "type": "object",
      "properties": {
        "key": {},
        "name": {},
        "title": {},
        "type": {},
        "round": {},
        "visible": {},
        "no_total": {},
        "max": {},
        "max_visible": {},
        "is_extra": {},
        "position": {},
        "landmark": {},
        "landmark_buttons": {}
      }
    },
    "runner": {
      "type": "object",
      "properties": {
        "name": {},
        "category": {},
        "title": {},
        "output_title": {},
        "disabled": {},
        "visible": {},
        "output_visible": {},
        "position": {},
        "timeout": {},
        "xterm_js": {},
        "timed_replay": {},
        "transfer_warnings": {},
        "transfer_warnings_priority": {},
        "command": {},
        "username": {},
        "overlay": {},
        "queue": {},
        "nconcurrent": {},
        "eval": {},
        "require": {}
      }
    },
    "diff": {
      "type": "object",
      "properties": {
        "regex": {},
        "title": {},
        "match_priority": {},
        "position": {},
        "fileless": {},
        "full": {},
        "ignore": {},
        "boring": {},
        "collate": {},
        "gradeable": {},
        "hide_if_anonymous": {}
      }
    },
    "download": {
      "type": "object",
      "properties": {
        "key": {},
        "title": {},
        "filename": {},
        "visible": {},
        "position": {},
        "file": {},
        "timed": {},
        "timeout": {}
      }
    }
  },
  "type": "object",
  "patternProperties": {
    "^[^_./\u0026;#][^/\u0026;#]*$": {
      "$ref": "#/definitions/$defs_pset"
    }
  },
  "properties": {
    "_defaults": {
      "$ref": "#/definitions/$defs_pset_base"
    }
  },
  "definitions": {
    "$defs_boolean_date": {
      "anyOf": [
        {
          "type": "boolean"
        },
        {
          "type": "integer",
          "exclusiveMinimum": 1000000000
        },
        {
          "type": "string"
        }
      ]
    },
    "$defs_pset_base": {
      "type": "object",
      "properties": {
        "psetid": {
          "description": "Unique database identifier for pset",
          "type": "integer",
          "exclusiveMinimum": 0
        },
        "key": {
          "description": "Unique string identifier for pset",
          "type": "string",
          "pattern": "^(?!.*_(?:rank|noextra|norm)$)[^_./\u0026;#][^/\u0026;#]*$"
        },
        "urlkey": {
          "description": "Preferred URL component for pset",
          "type": [
            "string",
            "integer"
          ],
          "pattern": "^[0-9A-Za-z][-0-9A-Za-z_.]*$"
        },
        "title": {
          "description": "Textual description of pset",
          "type": "string"
        },
        "category": {
          "description": "Name of pset group",
          "type": "string"
        },
        "weight": {
          "description": "Weight of pset within group",
          "type": "number",
          "minimum": 0
        },
        "disabled": {
          "description": "True if pset is not shown to users",
          "type": "boolean"
        },
        "admin_disabled": {
          "description": "True if pset is not shown to users or administrators",
          "type": "boolean"
        },
        "visible": {
          "description": "True if pset is shown to students",
          "$ref": "#/definitions/$defs_boolean_date"
        },
        "frozen": {
          "description": "True if students cannot update pset",
          "$ref": "#/definitions/$defs_boolean_date"
        },
        "anonymous": {},
        "gitless": {},
        "gitless_grades": {},
        "partner": {},
        "no_branch": {},
        "partner_repo": {},
        "hide_comments": {},
        "handout_repo_url": {},
        "handout_branch": {},
        "handout_hash": {},
        "handout_warn_hash": {},
        "directory": {},
        "test_file": {},
        "deadline": {},
        "grades": {},
        "grade_order": {},
        "grades_visible": {},
        "grade_statistics_visible": {},
        "grade_cdf_cutoff": {},
        "separate_extension_grades": {},
        "grade_script": {},
        "downloads": {},
        "runners": {},
        "default_runner": {},
        "runner_order": {},
        "run_dirpattern": {},
        "run_username": {},
        "run_overlay": {},
        "run_jailfiles": {},
        "run_xterm_js": {},
        "run_timeout": {},
        "run_skeletondir": {},
        "run_binddir": {},
        "diffs": {}
      }
    },
    "$defs_pset": {
      "$ref": "#/definitions/$defs_pset_base",
      "required": [
        "psetid",
        "key"
      ]
    }
  }
}