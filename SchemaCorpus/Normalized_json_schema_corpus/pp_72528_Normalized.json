                      "$ref": "#/definitions/definitions_coupon_definitions_voice"
                    },
                    "couponUse": {
                      "$ref": "#/definitions/definitions_base_definitions_couponUse"
                    },
                    "coupon": {
                      "type": [
                        "array"
                      ],
                      "items": {
                        "$ref": "#/definitions/definitions_coupon_definitions_coupon"
                      }
                    }
                  },
                  "required": [
                    "hdoServiceCode",
                    "number",
                    "iccid",
                    "regulation",
                    "sms",
                    "voice",
                    "couponUse",
                    "coupon"
                  ]
                }
              },
              "coupon": {
                "type": [
                  "array"
                ],
                "items": {
                  "$ref": "#/definitions/definitions_coupon_definitions_coupon"
                }
              }
