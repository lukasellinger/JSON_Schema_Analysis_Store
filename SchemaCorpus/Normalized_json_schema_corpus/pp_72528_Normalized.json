    "packet": {
      "$schema": "http://json-schema.org/draft-04/hyper-schema",
      "title": "IIJmio Coupon API - Packet",
      "definitions": {
        "date": {
          "description": "通信を行った日付",
          "example": "201403",
          "type": [
            "string"
          ]
        },
        "withCoupon": {
          "description": "クーポン使用状態でのデータ利用量（MB単位）",
