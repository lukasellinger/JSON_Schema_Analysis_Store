          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "marketingName": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "manufacturer": {
          "type": "string"
        },
        "brand": {
          "type": "string"
        },
        "display": {
