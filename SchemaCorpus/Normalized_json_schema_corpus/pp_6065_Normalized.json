{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://api.fuelrats.com/schema/rescue.json",
  "type": "object",
  "title": "Fuel Rats Rescue Schema",
  "required": [
    "type",
    "id",
    "attributes",
    "relationships",
    "links"
  ],
  "properties": {
    "type": {
      "const": "rescues"
    },
    "id": {
      "type": "string",
      "format": "uuid",
      "title": "The id Schema",
      "default": "",
      "examples": [
        "bacab9b8-02f2-4078-80c9-67af757944b8"
      ]
    },
    "attributes": {
      "type": "object",
      "title": "The Attributes Schema",
      "required": [
        "client",
        "codeRed",
        "data",
        "notes",
        "platform",
        "quotes",
        "status",
        "system",
        "title",
        "outcome",
        "unidentifiedRats",
        "createdAt",
        "updatedAt"
      ],
      "properties": {
        "client": {
          "type": [
            "string",
            "null"
          ],
          "title": "The Client Schema",
          "examples": [
            "Orange Sheets"
          ]
        },
        "codeRed": {
          "type": "boolean",
          "title": "The Code-red Schema",
          "default": false,
          "examples": [
            false
          ]
        },
        "data": {
          "type": "object",
          "title": "The Data Schema"
        },
        "notes": {
          "type": "string",
          "title": "The Notes Schema",
          "default": "",
          "examples": [
            "old entry"
          ],
          "pattern": "^(.*)$"
        },
        "platform": {
          "type": [
            "string",
            "null"
          ],
          "title": "The Platform Schema",
          "enum": [
            "pc",
            "xb",
            "ps",
            null
          ]
        },
        "quotes": {
          "type": "array",
          "title": "The Quotes Schema"
        },
        "status": {
          "type": "string",
          "title": "The Status Schema",
          "default": "open",
          "enum": [
            "open",
            "inactive",
            "closed"
          ]
        },
        "system": {
          "type": [
            "string",
            "null"
          ],
          "title": "The System Schema",
          "examples": [
            "Ashima"
          ],
          "pattern": "^(.*)$"
        },
        "title": {
          "type": [
            "string",
            "null"
          ],
          "title": "The Title Schema",
          "examples": [
            "Persera"
          ]
        },
        "outcome": {
          "type": [
            "string",
            "null"
          ],
          "title": "The Outcome Schema",
          "enum": [
            "success",
            "failure",
            "invalid",
            "other",
            null
          ]
        },
        "unidentifiedRats": {
          "type": "array",
          "title": "The Unidentifiedrats Schema"
        },
        "createdAt": {
          "type": "string",
          "title": "The createdAt Schema",
          "examples": [
            "2015-06-01T04:00:00.000Z"
          ],
          "pattern": "^(\\d{4}-[01]\\d-[0-3]\\dT[0-2]\\d:[0-5]\\d:[0-5]\\d\\.\\d+([+-][0-2]\\d:[0-5]\\d|Z))|(\\d{4}-[01]\\d-[0-3]\\dT[0-2]\\d:[0-5]\\d:[0-5]\\d([+-][0-2]\\d:[0-5]\\d|Z))|(\\d{4}-[01]\\d-[0-3]\\dT[0-2]\\d:[0-5]\\d([+-][0-2]\\d:[0-5]\\d|Z))$"
        },
        "updatedAt": {
          "type": "string",
          "title": "The updatedAt Schema",
          "examples": [
            "2017-09-08T19:16:46.878Z"
          ],
          "pattern": "^(\\d{4}-[01]\\d-[0-3]\\dT[0-2]\\d:[0-5]\\d:[0-5]\\d\\.\\d+([+-][0-2]\\d:[0-5]\\d|Z))|(\\d{4}-[01]\\d-[0-3]\\dT[0-2]\\d:[0-5]\\d:[0-5]\\d([+-][0-2]\\d:[0-5]\\d|Z))|(\\d{4}-[01]\\d-[0-3]\\dT[0-2]\\d:[0-5]\\d([+-][0-2]\\d:[0-5]\\d|Z))$"
        }
      }
    },
    "relationships": {
      "type": "object",
      "title": "The Relationships Schema",
      "required": [
        "rats",
        "firstLimpet"
      ],
      "properties": {
        "rats": {
          "type": "object",
          "title": "The Rats Schema",
          "required": [
            "links"
          ],
          "properties": {
            "links": {
              "type": "object",
              "title": "The Links Schema",
              "required": [
                "self",
                "related"
              ],
              "properties": {
                "self": {
                  "type": "string",
                  "title": "The Self Schema",
                  "default": "",
                  "examples": [
                    "http://localhost:8082/rescues/bacab9b8-02f2-4078-80c9-67af757944b8/relationships/rats"
                  ],
                  "pattern": "^(.*)$"
                },
                "related": {
                  "type": "string",
                  "title": "The Related Schema",
                  "default": "",
                  "examples": [
                    "http://localhost:8082/rescues/bacab9b8-02f2-4078-80c9-67af757944b8/rats"
                  ],
                  "pattern": "^(.*)$"
                }
              }
            },
            "data": {
              "type": "array",
              "title": "The Data Schema",
              "items": {
                "type": "object",
                "title": "The Items Schema",
                "required": [
                  "type",
                  "id"
                ],
                "properties": {
                  "type": {
                    "type": "string",
                    "title": "The Type Schema",
                    "default": "",
                    "examples": [
                      "rats"
                    ],
                    "pattern": "^(.*)$"
                  },
                  "id": {
                    "type": "string",
                    "title": "The Id Schema",
                    "default": "",
                    "examples": [
                      "97497d03-3c7d-47d3-9714-fa85452e6d97"
                    ],
                    "pattern": "^(.*)$"
                  }
                }
              }
            }
          }
        },
        "firstLimpet": {
          "type": "object",
          "title": "The Firstlimpet Schema",
          "required": [
            "links"
          ],
          "properties": {
            "links": {
              "type": "object",
              "title": "The Links Schema",
              "required": [
                "self",
                "related"
              ],
              "properties": {
                "self": {
                  "type": "string",
                  "title": "The Self Schema",
                  "default": "",
                  "examples": [
                    "http://localhost:8082/rescues/bacab9b8-02f2-4078-80c9-67af757944b8/relationships/firstLimpet"
                  ],
                  "pattern": "^(.*)$"
                },
                "related": {
                  "type": "string",
                  "title": "The Related Schema",
                  "default": "",
                  "examples": [
                    "http://localhost:8082/rescues/bacab9b8-02f2-4078-80c9-67af757944b8/firstLimpet"
                  ],
                  "pattern": "^(.*)$"
                }
              }
            },
            "data": {
              "type": "object",
              "title": "The Data Schema",
              "required": [
                "type",
                "id"
              ],
              "properties": {
                "type": {
                  "type": "string",
                  "title": "The Type Schema",
                  "default": "",
                  "examples": [
                    "rats"
                  ],
                  "pattern": "^(.*)$"
                },
                "id": {
                  "type": "string",
                  "title": "The Id Schema",
                  "default": "",
                  "examples": [
                    "97497d03-3c7d-47d3-9714-fa85452e6d97"
                  ],
                  "pattern": "^(.*)$"
                }
              }
            }
          }
        }
      }
    },
    "links": {
      "type": "object",
      "title": "The Links Schema",
      "required": [
        "self"
      ],
      "properties": {
        "self": {
          "type": "string",
          "title": "The Self Schema",
          "default": "",
          "examples": [
            "http://localhost:8082/rescues/bacab9b8-02f2-4078-80c9-67af757944b8"
          ],
          "pattern": "^(.*)$"
        }
      }
    }
  }
}