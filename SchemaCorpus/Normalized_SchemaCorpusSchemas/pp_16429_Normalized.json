{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "flac",
  "name": "FLAC",
  "defaults": {},
  "groups": [
    {
      "class": "basic",
      "properties": [
        {
          "parameter": "_sampleFormat",
          "forced": true,
          "control": {
            "type": "combobox",
            "selectedIndex": 0,
            "items": [
              {
                "value": "s16"
              },
              {
                "value": "s32"
              }
            ]
          }
        }
      ]
    },
    {
      "class": "advanced",
      "properties": [
        {
          "parameter": "compression_level",
          "control": {
            "maximum": 12,
            "minimum": 0,
            "singleStep": 1,
            "type": "integer",
            "value": 5
          }
        }
      ]
    }
  ]
}