{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "iOS",
    "hostMachines"
  ],
  "properties": {
    "iOS": {
      "type": "object",
      "title": "The Ios Schema"
    },
    "hostMachines": {
      "type": "array",
      "title": "The Hostmachines Schema",
      "items": {
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "machineIP"
        ],
        "properties": {
          "machineIP": {
            "type": [
              "array",
              "string"
            ],
            "title": "The Machineip Schema",
            "default": "",
            "examples": [
              "127.0.0.1"
            ],
            "pattern": "^(.*)$"
          },
          "appiumServerPath": {
            "type": "string",
            "title": "The Appiumserverpath Schema",
            "default": "",
            "examples": [
              "/usr/local/lib/node_modules/appium/build/lib/main.js"
            ],
            "pattern": "^(.*)$"
          },
          "simulators": {
            "type": "array",
            "title": "The Simulators Schema",
            "items": {
              "type": "object",
              "title": "The Items Schema",
              "required": [
                "deviceName",
                "OS"
              ],
              "properties": {
                "deviceName": {
                  "type": "string",
                  "title": "The Devicename Schema",
                  "default": "",
                  "examples": [
                    "iPhone 6"
                  ],
                  "pattern": "^(.*)$"
                },
                "OS": {
                  "type": "string",
                  "title": "The Os Schema",
                  "default": "",
                  "examples": [
                    "11.4"
                  ],
                  "pattern": "^(.*)$"
                }
              }
            }
          }
        }
      }
    },
    "ATDServiceHost": {
      "type": "string",
      "title": "The Atdservicehost Schema",
      "default": "",
      "examples": [
        "127.0.0.1"
      ],
      "pattern": "^(.*)$"
    },
    "ATDServicePort": {
      "type": "string",
      "title": "The Atdserviceport Schema",
      "default": "",
      "examples": [
        "3000"
      ],
      "pattern": "^(.*)$"
    }
  }
}