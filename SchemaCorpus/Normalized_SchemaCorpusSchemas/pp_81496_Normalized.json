{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://integracja.firefighters.ki.agh.edu.pl",
  "description": "RQ to firefightersPost",
  "type": "object",
  "properties": {
    "notificationId": {
      "javaType": "Long"
    },
    "type": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "actionName": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "latitude": {
      "type": "string"
    },
    "longitude": {
      "type": "string"
    },
    "notifiactionDate": {
      "format": "date-time"
    },
    "creator": {
      "type": "string"
    },
    "orderedResources": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "quantity": {
            "type": "integer"
          }
        },
        "additionalProperties": false
      },
      "additionalItems": false
    },
    "requestedFirefightersUnitNumber": {
      "type": "integer"
    }
  },
  "additionalProperties": false
}