{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "nullable": "false",
  "id": "http://wso2jsonschema.org",
  "title": "soapenv:Envelope",
  "type": "object",
  "properties": {
    "soapenv:Body": {
      "type": "object",
      "nullable": "false",
      "properties": {
        "ns1:employees": {
          "type": "object",
          "nullable": "false",
          "properties": {
            "ns1:employee": {
              "type": "array",
              "nullable": "false",
              "items": [
                {
                  "type": "object",
                  "properties": {
                    "ns1:firstname": {
                      "type": "string",
                      "nullable": "false"
                    },
                    "ns1:lastname": {
                      "type": "string",
                      "nullable": "false"
                    },
                    "ns2:addresses": {
                      "type": "object",
                      "nullable": "false",
                      "properties": {
                        "ns2:address": {
                          "type": "array",
                          "nullable": "false",
                          "items": [
                            {
                              "type": "object",
                              "properties": {
                                "ns2:city": {
                                  "type": "object",
                                  "value": {
                                    "type": "string"
                                  },
                                  "nullable": "false",
                                  "properties": {},
                                  "attributes": {
                                    "postalcode": {
                                      "type": "number",
                                      "nullable": "false"
                                    }
                                  }
                                },
                                "ns2:road": {
                                  "type": "string",
                                  "nullable": "false"
                                }
                              },
                              "attributes": {
                                "location": {
                                  "type": "string",
                                  "nullable": "false"
                                }
                              }
                            }
                          ]
                        }
                      }
                    }
                  }
                }
              ]
            }
          }
        }
      }
    }
  },
  "namespaces": [
    {
      "prefix": "soapenv",
      "url": "http://schemas.xmlsoap.org/soap/envelope/"
    },
    {
      "prefix": "ns1",
      "url": "http://wso2.employee.info"
    },
    {
      "prefix": "ns2",
      "url": "http://wso2.employee.address"
    }
  ]
}