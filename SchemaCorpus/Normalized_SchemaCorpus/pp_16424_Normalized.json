{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "alac",
  "name": "ALAC",
  "defaults": {
    "_sampleFormat": "s16p"
  },
  "groups": [
    {
      "class": "advanced",
      "properties": [
        {
          "parameter": "min_prediction_order",
          "control": {
            "type": "integer",
            "minimum": 1,
            "maximum": 30,
            "singleStep": 1,
            "value": 4
          },
          "filters": [
            {
              "filter": "OnChangeValue",
              "params": {
                "LimitMaxValue": [
                  {}
                ]
              }
            }
          ]
        },
        {
          "parameter": "max_prediction_order",
          "control": {
            "type": "integer",
            "minimum": 1,
            "maximum": 30,
            "singleStep": 1,
            "value": 6
          },
          "filters": [
            {
              "filter": "OnChangeValue",
              "params": {
                "LimitMinValue": [
                  {}
                ]
              }
            }
          ]
        }
      ]
    }
  ]
}