{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://dcc.icgc.org/donor-centric",
  "type": "object",
  "properties": {
    "_donor_id": {
      "type": "string"
    },
    "_project_id": {
      "type": "string"
    },
    "_summary": {
      "type": "object",
      "properties": {
        "_affected_gene_count": {
          "type": "integer"
        },
        "_age_at_diagnosis_group": {
          "type": "string"
        },
        "_available_data_type": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "additionalItems": false
        },
        "_cngv_exists": {
          "type": "boolean"
        },
        "_cnsm_exists": {
          "type": "boolean"
        },
        "_exp_array_exists": {
          "type": "boolean"
        },
        "_exp_seq_exists": {
          "type": "boolean"
        },
        "_jcn_exists": {
          "type": "boolean"
        },
        "_meth_array_exists": {
          "type": "boolean"
        },
        "_meth_seq_exists": {
          "type": "boolean"
        },
        "_mirna_seq_exists": {
          "type": "boolean"
        },
        "_pexp_exists": {
          "type": "boolean"
        },
        "_sgv_exists": {
          "type": "boolean"
        },
        "_ssm_count": {
          "type": "integer"
        },
        "_state": {
          "type": "string"
        },
        "_stgv_exists": {
          "type": "boolean"
        },
        "_stsm_exists": {
          "type": "boolean"
        },
        "_studies": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "additionalItems": false
        },
        "experimental_analysis_performed": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "additionalItems": false
        },
        "experimental_analysis_performed_sample_count": {
          "type": "object",
          "description": "Each field of the object has name of experimental analysis performed and it\u0027s count. E.g. WGA:10",
          "patternProperties": {
            "^\\w+(-\\w+)+$": {
              "type": "integer"
            }
          },
          "additionalProperties": false
        },
        "repository": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "additionalItems": false
        }
      },
      "additionalProperties": false
    },
    "disease_status_last_followup": {
      "type": "string"
    },
    "donor_age_at_diagnosis": {
      "type": "integer"
    },
    "donor_relapse_type": {
      "type": "string"
    },
    "donor_sex": {
      "type": "string"
    },
    "donor_survival_time": {
      "type": "integer"
    },
    "donor_tumour_stage_at_diagnosis": {
      "type": "string"
    },
    "donor_vital_status": {
      "type": "string"
    },
    "gene": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "_gene_id": {
            "type": "string"
          },
          "biotype": {
            "type": "string"
          },
          "chromosome": {
            "type": "string"
          },
          "curated_set": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "additionalItems": false
          },
          "drug": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "additionalItems": false
          },
          "end": {
            "type": "integer"
          },
          "fake": {
            "description": "This is present only when the value is \u0027true\u0027",
            "type": "boolean"
          },
          "go_term": {
            "type": "object",
            "properties": {
              "biological_process": {
                "type": "array",
                "items": {
                  "type": "string"
                },
                "additionalItems": false
              },
              "cellular_component": {
                "type": "array",
                "items": {
                  "type": "string"
                },
                "additionalItems": false
              },
              "molecular_function": {
                "type": "array",
                "items": {
                  "type": "string"
                },
                "additionalItems": false
              }
            },
            "additionalProperties": false
          },
          "pathway": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "additionalItems": false
          },
          "ssm": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "_mutation_id": {
                  "type": "string"
                },
                "_type": {
                  "type": "string"
                },
                "chromosome": {
                  "type": "string"
                },
                "chromosome_end": {
                  "type": "integer"
                },
                "chromosome_start": {
                  "type": "integer"
                },
                "consequence": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "consequence_type": {
                        "type": "string"
                      },
                      "functional_impact_prediction_summary": {
                        "type": "string"
                      }
                    },
                    "additionalProperties": false
                  },
                  "additionalItems": false
                },
                "mutation_type": {
                  "type": "string"
                },
                "observation": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "verification_status": {
                        "type": "string"
                      },
                      "platform": {
                        "type": "string"
                      },
                      "sequencing_strategy": {
                        "type": "string"
                      }
                    },
                    "additionalProperties": false
                  },
                  "additionalItems": false
                }
              },
              "additionalProperties": false
            },
            "additionalItems": false
          },
          "start": {
            "type": "integer"
          },
          "symbol": {
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "additionalItems": false
    },
    "project": {
      "type": "object",
      "properties": {
        "_project_id": {
          "type": "string"
        },
        "primary_site": {
          "type": "string"
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false,
  "required": [
    "_donor_id"
  ]
}