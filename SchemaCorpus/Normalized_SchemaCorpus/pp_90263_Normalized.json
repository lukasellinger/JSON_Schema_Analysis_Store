                  "type": "pointCloudFixedSizeAlgorithm",
                  "useRealWorldSymbolSizes": false,
                  "size": 4
                },
                "pointsPerInch": 10.0,
                "field": "CLASS_CODE",
                "colorUniqueValueInfos": [
                  {
                    "values": [
                      "1"
                    ],
                    "label": "1",
                    "description": "Unassigned",
                    "color": [
                      79,
