{
  "id": "http://baseurl.com/schemas",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "errorType": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "default": ""
      },
      "sex": {
        "enum": [
          "Male",
          "Female",
          "Other"
        ]
      }
    },
    "required": [
      "name",
      "sex"
    ]
  }
}