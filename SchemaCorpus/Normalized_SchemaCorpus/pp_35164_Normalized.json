{
  "$schema": "http://json-schema.org/schema#",
  "properties": {
    "address": {
      "items": {
        "$ref": "#/definitions/elements\\address.json"
      },
      "type": "array",
      "uniqueItems": true
    },
    "closed_date": {
      "format": "date",
      "title": "Date when the job was closed",
      "type": "string"
    },
    "contact_details": {
      "items": {
        "$ref": "#/definitions/elements\\contact.json"
      },
      "type": "array",
      "uniqueItems": true
    },
    "deadline_date": {
      "format": "date",
      "title": "Application deadline",
      "type": "string"
    },
    "deleted": {
      "type": "boolean"
    },
    "description": {
      "title": "Job description",
      "type": "string"
    },
    "experiments": {
      "items": {
        "properties": {
          "curated_relation": {
            "type": "boolean"
          },
          "name": {
            "title": "Experiment name",
            "type": "string"
          },
          "record": {
            "$ref": "#/definitions/elements\\json_reference.json",
            "title": "Experiment Record URI"
          }
        },
        "title": "Experiments associated with Job",
        "type": "object"
      },
      "type": "array",
      "uniqueItems": true
    },
    "field_categories": {
      "items": {
        "$ref": "#/definitions/elements\\field.json"
      },
      "type": "array",
      "uniqueItems": true
    },
    "institutions": {
      "items": {
        "properties": {
          "curated_relation": {
            "type": "boolean"
          },
          "name": {
            "title": "Institution name",
            "type": "string"
          },
          "record": {
            "$ref": "#/definitions/elements\\json_reference.json",
            "title": "Institution Record URI"
          }
        },
        "title": "Institution",
        "type": "object"
      },
      "type": "array",
      "uniqueItems": true
    },
    "position": {
      "title": "Job position",
      "type": "string"
    },
    "ranks": {
      "items": {
        "$ref": "#/definitions/elements\\rank.json"
      },
      "type": "array",
      "uniqueItems": true
    },
    "reference_email": {
      "items": {
        "description": "FIXME: what is the difference from contact_email!?",
        "format": "email",
        "title": "Reference email",
        "type": "string"
      },
      "type": "array",
      "uniqueItems": true
    },
    "regions": {
      "items": {
        "enum": [
          "Africa",
          "Australasia",
          "Asia",
          "Europe",
          "Middle East",
          "North America",
          "South America"
        ],
        "type": "string"
      },
      "type": "array"
    },
    "self": {
      "$ref": "#/definitions/elements\\json_reference.json",
      "description": "Url of the record itself",
      "title": "Url of the record"
    },
    "urls": {
      "items": {
        "$ref": "#/definitions/elements\\url.json"
      },
      "type": "array",
      "uniqueItems": true
    }
  },
  "title": "HEP Job",
