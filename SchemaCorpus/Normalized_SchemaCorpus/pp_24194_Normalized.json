{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Collection of log data transfer objects",
  "type": "object",
  "properties": {
    "logDtos": {
      "description": "List of log data transfer objects",
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "#/definitions/logDto.json"
      }
    },
    "totalRecords": {
      "type": "integer"
    }
  },
  "required": [
    "logDtos",
    "totalRecords"
  ],
  "definitions": {
    "logDto.json": {
      "$schema": "http://json-schema.org/draft-04/schema#",
      "description": "Log Schema",
      "type": "object",
      "properties": {
        "jobExecutionId": {
          "description": "Id, Unique identifier of the executed job",
          "type": "string"
        },
        "jobExecutionHrId": {
          "description": "Human readable id of the executed job",
          "type": "string"
        },
        "jobProfileName": {
          "description": "Name of applied job profile",
          "type": "string"
        },
        "fileName": {
          "description": "File name with extension",
          "type": "string"
        },
        "runBy": {
          "description": "First and last name of the user that triggered the job execution",
          "type": "object",
          "properties": {
            "firstName": {
              "description": "First name",
              "type": "string"
            },
            "lastName": {
              "description": "Last name",
              "type": "string"
            }
          }
        },
        "completedDate": {
          "description": "Date and time when completed the job execution",
          "type": "string",
          "format": "date-time"
        },
        "status": {
          "description": "Current status of the job execution",
          "type": "string",
          "$ref": "#/definitions/..\\mod-source-record-manager\\status.json"
        },
        "uiStatus": {
          "description": "Status that is rendered on UI",
          "type": "string",
          "$ref": "#/definitions/..\\mod-source-record-manager\\uiStatus.json"
        }
      },
      "required": [
        "jobExecutionId",
        "jobExecutionHrId",
        "jobProfileName",
        "runBy",
        "completedDate"
      ]
    },
    "..\\mod-source-record-manager\\status.json": {
      "$schema": "http://json-schema.org/draft-04/schema#",
      "description": "JobExecution status Enum",
      "type": "string",
      "enum": [
        "PARENT",
        "NEW",
        "FILE_UPLOADED",
        "IMPORT_IN_PROGRESS",
        "IMPORT_FINISHED",
        "PARSING_IN_PROGRESS",
        "PARSING_FINISHED",
        "PROCESSING_IN_PROGRESS",
        "PROCESSING_FINISHED",
        "COMMIT_IN_PROGRESS",
        "COMMITTED",
        "ERROR",
        "DISCARDED"
      ]
    },
    "..\\mod-source-record-manager\\uiStatus.json": {
      "$schema": "http://json-schema.org/draft-04/schema#",
      "description": "JobExecution UI status Enum",
      "type": "string",
      "enum": [
        "PARENT",
        "INITIALIZATION",
        "PREPARING_FOR_PREVIEW",
        "READY_FOR_PREVIEW",
        "RUNNING",
        "RUNNING_COMPLETE",
        "ERROR",
        "DISCARDED"
      ]
    }
  }
}