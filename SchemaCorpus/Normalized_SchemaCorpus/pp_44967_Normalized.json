{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Base schema for all Senpy objects",
  "type": "object",
  "properties": {
    "@id": {
      "type": "string"
    },
    "@type": {
      "type": "string",
      "description": "Type of the atom. e.g., \u0027onyx:EmotionAnalysis\u0027, \u0027nif:Entry\u0027"
    }
  },
  "required": [
    "@id",
    "@type"
  ]
}