{
  "name": "Strict Contract",
  "request": {
    "http_method": "GET",
    "path": "/strict",
    "headers": {
      "Accept": "application/json"
    },
    "params": {}
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "schema": {
      "$schema": "http://json-schema.org/draft-03/schema#",
      "type": "object",
      "required": true,
      "properties": {
        "devices": {
          "type": "array",
          "minItems": 2,
          "items": {
            "type": "string",
            "required": true,
            "default": "/dev/\u003c%\u003d values[:device_id].tap do values[:device_id] \u003d values[:device_id] + 1 end %\u003e",
            "pattern": "^/dev/[\\d]+$"
          },
          "required": true,
          "uniqueItems": true
        }
      }
    }
  }
}