{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://kddart.org",
  "type": "object",
  "properties": {
    "DATA": {
      "type": "array",
      "items": {
        "oneOf": [
          {
            "type": "object",
            "properties": {
              "ItemId": {
                "type": "integer"
              },
              "SpecimenId": {
                "type": "integer"
              },
              "ItemTypeId": {
                "type": "integer"
              },
              "ItemBarcode": {
                "type": "string"
              },
              "LastUpdateTimeStamp": {
                "type": "string"
              },
              "Amount": {
                "type": "number"
              },
              "TrialUnitSpecimenId": {
                "type": "integer"
              },
              "ItemSourceId": {
                "type": "integer"
              },
              "ContainerTypeId": {
                "type": "integer"
              },
              "ScaleId": {
                "type": "integer"
              },
              "StorageId": {
                "type": "integer"
              },
              "UnitId": {
                "type": "integer"
              },
              "ItemStateId": {
                "type": "integer"
              },
              "LastMeasuredDate": {
                "type": "string"
              },
              "LastMeasuredUserId": {
                "type": "string"
              },
              "ItemOperation": {
                "type": "string"
              },
              "ItemNote": {
                "type": "string"
              }
            },
            "required": [
              "ItemId",
              "SpecimenId",
              "ItemTypeId",
              "LastUpdateTimeStamp"
            ],
            "additionalProperties": true
          }
        ]
      },
      "minItems": 1
    }
  },
  "required": [
    "DATA"
  ]
}