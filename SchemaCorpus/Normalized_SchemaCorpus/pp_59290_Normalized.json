{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://jsonschema.net",
  "type": "object",
  "properties": {
    "identityId": {
      "type": "string"
    },
    "token": {
      "type": "string"
    },
    "credentials": {
      "type": "object",
      "properties": {
        "accessKey": {
          "type": "string"
        },
        "secretKey": {
          "type": "string"
        },
        "sessionToken": {
          "type": "string"
        },
        "expiration": {
          "type": "integer"
        }
      }
    }
  },
  "required": [
    "identityId",
    "token",
    "credentials"
  ]
}