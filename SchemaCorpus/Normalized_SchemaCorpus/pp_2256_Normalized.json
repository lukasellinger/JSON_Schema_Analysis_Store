{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://localhost:9999/schemas/ticket.json",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "message": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "priority": {
      "enum": [
        "low",
        "medium",
        "high"
      ]
    }
  },
  "required": [
    "id",
    "message",
    "url",
    "priority"
  ]
}